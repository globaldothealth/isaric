{
  "name": "isaric-ccpuk",
  "description": "ISARIC CCPUK study",
  "tables": {
    "study": {
      "kind": "constant"
    },
    "subject": {
      "kind": "groupBy",
      "groupBy": "subject_id",
      "aggregation": "lastNotNull"
    },
    "visit": {
      "kind": "groupBy",
      "groupBy": "visit_id",
      "aggregation": "lastNotNull"
    }
  },
